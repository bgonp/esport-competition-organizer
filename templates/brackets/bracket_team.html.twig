{% set team = round.teams|length > team_index ? round.teams[team_index] : null %}
<a data-round="{{ round.id }}" data-team="{{ team ? team.id : '' }}"
   class="{{ clickable ? 'bracket-button' : '' }} {{ round.winner ? round.winner == team ? 'win' : 'lose' : '' }}"
   {% if clickable %}href="#" {% endif %}title="{{ team ? team.name : '' }}">{{ team ? team.name : '' }}</a>